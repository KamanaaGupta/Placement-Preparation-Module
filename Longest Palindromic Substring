class Solution:
    def longestPalindrome(self, s: str) -> str:
        palin=""
        for i in range(len(s)):
            x=""
            for j in range(i,len(s)):
                x=x+s[j]
                y=x[::-1]
                if y==x and len(y)>len(palin):
                    palin=x
        return palin
                
